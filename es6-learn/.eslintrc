{
  "ecmaFeatures": {
    "jsx": true,
    "modules": true
  },
  "settings": {
    "ecmascript": 6,
    "jsx": true
  },
  "env": {
    "browser": true,
    "node": true
  },
  "parser": "babel-eslint",
  "rules": {
    "quotes": [2, "single"],
    "strict": [2, "never"],
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2,
    "react/react-in-jsx-scope": 2
    "camelcase": 0,
    "no-unused-vars": 0
  },
  "plugins": [
    "react"
  ]
}
